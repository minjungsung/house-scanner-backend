{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run gRPC server",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/mindtests/main.py",
            "console": "integratedTerminal",
            "envFile": "${workspaceFolder}/mindtests/.env",
            "cwd": "${workspaceFolder}/mindtests",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/mindtests/pyproto:${workspaceFolder}/mindtests:$PYTHONPATH"
            }
        },
    ]
}